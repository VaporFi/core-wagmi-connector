{
  "name": "@vaporfi/core-wagmi-connector",
  "version": "1.0.2",
  "description": "An injected connector for core wallet.",
  "keywords": [
    "wagmi",
    "ethers",
    "ethereum"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VaporFi/core-wagmi-connector.git"
  },
  "license": "MIT",
  "author": "Hitesh Agrawal (https://github.com/Thehitesh172)",
  "contributors": [
    {
      "name": "Jose Mejias"
    }
  ],
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --clean --dts --format cjs,esm --external ethers,wagmi",
    "prepare": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "ethers": "5.7.2",
    "typescript": "4.9.3",
    "wagmi": "0.8.10"
  },
  "devDependencies": {
    "tsup": "6.5.0"
  },
  "peerDependencies": {
    "ethers": "^5.7.2",
    "wagmi": "^0.8.10"
  },
  "engines": {
    "node": ">=16"
  }
}
